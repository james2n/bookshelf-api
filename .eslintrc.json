{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base", "prettier"
    ],
    "plugins": ["prettier"],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "no-console": "off",
        "prettier/prettier": "error"
        // "function-paren-newline": ["error", "never"],
        // "no-unused-vars": "error"
        // "function-paren-newline": ["error", { "minItems": 3 }]
    }
}
